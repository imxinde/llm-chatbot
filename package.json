{
  "name": "llm-chatbot",
  "version": "1.0.0",
  "description": "A simple LLM ChatBot application using OpenRouter API",
  "type": "module",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev:server": "node --watch src/server.js",
    "dev:client": "cd client && npm run dev",
    "dev": "npm run dev:server & npm run dev:client",
    "build": "cd client && npm run build",
    "preview": "cd client && npm run preview"
  },
  "keywords": ["chatbot", "llm", "openrouter", "express"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "dotenv": "^16.3.1"
  }
}
